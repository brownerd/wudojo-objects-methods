{
  "name": "taping",
  "version": "1.0.0",
  "description": "unit testing common scenarios with tape",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "cov": "npm run cov:clean && npm run cov:generate",
    "cov:clean": "rimraf coverage",
    "cov:generate": "babel-node node_modules/.bin/isparta cover --report text --report html test/index.js",
    "cover-unused": "babel-node node_modules/.bin/isparta cover --report text --report html ./test/*_tape.js",
    "dev": "watch 'clear && npm run -s lint && npm -s test' src test",
    "lint": "eslint src test",
    "start": "babel-node file-lister.js && webpack-dev-server --inline --hot --config webpack.config.js -f",
    "test": "babel-node ./test/index.js | faucet",
    "validate": "npm outdated --depth 0"
  },
  "author": "Marco Romero",
  "license": "MIT",
  "dependencies": {
    "babel": "^5.8.21",
    "babel-core": "^5.8.22"
  },
  "devDependencies": {
    "babel-core": "6.3.15",
    "babel-eslint": "4.1.6",
    "babel-loader": "6.2.0",
    "babel-plugin-transform-runtime": "6.3.13",
    "babel-polyfill": "6.3.14",
    "babel-preset-es2015": "6.3.13",
    "babel-preset-stage-0": "6.3.13",
    "babel-runtime": "6.3.13",
    "chalk": "1.1.1",
    "css-loader": "0.23.0",
    "eslint": "1.10.3",
    "faucet": "0.0.1",
    "file-loader": "0.8.5",
    "html-webpack-plugin": "1.7.0",
    "isparta": "4.0.0",
    "open-browser-webpack-plugin": "0.0.1",
    "style-loader": "0.13.0",
    "tape": "4.2.2",
    "url-loader": "0.5.7",
    "watch": "0.16.0",
    "webpack": "1.12.9",
    "webpack-dev-server": "1.14.0",
    "webpack-jsdom-tape-plugin": "1.2.0",
    "yargs": "3.31.0"
  }
}
